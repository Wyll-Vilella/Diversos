Algoritmo "Creuza01"


Var
   ano_atual, ano_nasc , idade : inteiro
Inicio
      escreva ( " Em que ano nós estamos? ")
      Leia ( ano_atual)
      escreva (" Em que ano nasci? " )
      Leia ( ano_nasc )
      idade <- ano_atual - ano_nasc
      escreva (" A sua idade é :" , idade)
      

Fimalgoritmo