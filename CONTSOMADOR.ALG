Algoritmo "CONTsomador"

Var
cont , n , s : inteiro

Inicio
      cont <- 1
 Enquanto ( cont <= 5 ) faca
      EscrevaL ("Digite o " , cont , " °. valor: ")
      Leia (n)
      s <- s + n
      cont <- cont + 1
 Fimenquanto
 EscrevaL (" A soma de todos os valores foi ",s)
 

Fimalgoritmo