Algoritmo "conversormoedasDólar"

Var  R , D , C : real


Inicio
      C <- 1
      Enquanto ( C <=10) faca
               Escreval ( "Qual o valor em Reais?" )
               Leia (R)
               D <- R/5.50
               Escreva (" O valor em dólar é:" , D:5:2)
      
      
      FimEnquanto
      

Fimalgoritmo